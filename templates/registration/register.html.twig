{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}
{% block body %}
    <form>
{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.password) }}
    {{ form_row(form.email) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    </form>

<button type="submit">Register!</button>
{{ form_end(form) }}
{% endblock %}